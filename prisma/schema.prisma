datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Produto {
  id        String   @id @default(uuid())
  nome      String   @unique
  descricao String
  preco     Float
  imagemUrl String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
